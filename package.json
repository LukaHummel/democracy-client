{
  "name": "democracyclient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "react-native run-ios",
    "start:android": "react-native run-android",
    "start:web": "node ./scripts/webpack.dev.js",
    "build:ios": "cd ios && xcodebuild -quiet -scheme democracyclient -configuration Release -sdk iphonesimulator",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:web": "react-scripts build",
    "clean:android": "cd android && ./gradlew clean",
    "test:eslint": "eslint ./src"
  },
  "dependencies": {
    "npm": "^5.5.1",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-dom": "^16.2.0",
    "react-native": "0.51.0",
    "react-native-elements": "^0.18.5",
    "react-native-navigation": "^1.1.304",
    "react-native-vector-icons": "^4.4.2",
    "react-native-web": "^0.1.16",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "styled-components": "^2.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-preset-react-native": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-native": "^3.2.0",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.2.1",
    "react-native-cli": "^2.0.1",
    "react-test-renderer": "^16.0.0",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
